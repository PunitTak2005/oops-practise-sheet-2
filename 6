#include <iostream>
using namespace std;
class Person {
public:
 Person() { // Constructor
 cout << "Constructor called!" << endl;
 }
 ~Person() { // Destructor
 cout << "Destructor called!" << endl;
 }
};
int main() {
 Person obj; // Constructor is called
 return 0; // Destructor is called automatically at end of scope
}
